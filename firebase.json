{
	"hosting": {
		"site": "itstambayan",
		"public": "public",
		"ignore": [
			"public/.htaccess",
			"public/access_log",
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [ {
			"source": "**",
			"destination": "/index.html"
		}],
		"headers": [ {
			"source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
			"headers": [ {
				"key": "Access-Control-Allow-Origin",
				"value": "*"
			}]
		}]
	},
	"database": {
		"rules": "database.rules.json"
	}
}